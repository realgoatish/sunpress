{
	"name": "sunpress",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"rimraf": "rimraf temp",
		"imageminsvgo": "node imagemin-svgo.js",
		"prebuildsprite": "npm run imageminsvgo",
		"buildsprite": "svgstore -o static/svg/sprite/sprite.svg temp/svg/*.svg --inline",
		"embedsprite": "node embed-sprite.js",
		"start": "npm run buildsprite && npm run embedsprite && npm run rimraf"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "^1.0.0-next.75",
		"@sveltejs/kit": "1.0.0",
		"@types/react": "^18.2.6",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"imagemin": "^8.0.1",
		"imagemin-svgo": "^10.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svgstore": "^3.0.1",
		"svgstore-cli": "^2.0.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@portabletext/svelte": "^2.0.0",
		"@sanity/client": "^3.4.1",
		"@sanity/groq-store": "^2.1.0",
		"@sanity/ui": "^1.3.3",
		"@sanity/vision": "^3.10.1",
		"groq": "^3.10.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sanity": "^3.11.1",
		"slug": "^8.0.0",
		"somerset": "^0.0.3",
		"styled-components": "^5.3.10",
		"svelte-every-layout": "^0.0.25",
		"tusculum": "^0.0.17"
	}
}
